{
  "name": "api-dummy",
  "tags": [
    "build-type:cdk"
  ],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "pnpm build",
        "cwd": "{workspaceRoot}/{projectRoot}"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo '{projectName} tests are running...' && pnpm test",
        "cwd": "{workspaceRoot}/{projectRoot}"
      }
    }
  }
}